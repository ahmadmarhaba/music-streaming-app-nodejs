// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Music {
  id           Int    @id @default(autoincrement())
  author       String
  authorImage  String
  albumName    String
  songName     String
  songImage    String
  songUrl      String
  songDuration Float
  songSize     Float
  songMimeType String
  cloudinaryId String
}
